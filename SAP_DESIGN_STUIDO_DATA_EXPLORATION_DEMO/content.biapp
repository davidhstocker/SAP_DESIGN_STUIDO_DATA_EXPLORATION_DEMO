});
if (filtertext.length > 0) 
{
 	   INFO_CONTENT.setHtmlText(INFO_CONTENT.getHtmlText() + filtertext);
}     
else { INFO_CONTENT.setHtmlText(INFO_CONTENT.getHtmlText() + TEXT_POOL.NoStaticFilterValue);
} 


// Varibales
INFO_CONTENT.setHtmlText(INFO_CONTENT.getHtmlText() + "<h3>" + TEXT_POOL.Variables + "</h3>");
