				<bi:property name="FUNCTION" index="0">
					<bi:property name="NAME" value="on_startup"/>
					<bi:property name="FUNCTION_BODY">
						<bi:value><![CDATA[
	

/* Set Remaining Filter values in case of Navigation */

g_display_mode = "CHART_TABLE";

