		<bi:component name="HELP_FUNCTIONS" type="GLOBAL_SCRIPTS_COMPONENT">
			<bi:property name="FUNCTIONS" type="ORDEREDLIST">
				<bi:property name="FUNCTION" index="0">
					<bi:property name="NAME" value="nav_panel_action"/>
					<bi:property name="FUNCTION_BODY">
						<bi:value><![CDATA[if (g_navigation_on) {
	PANEL_BODY_LEFT.setVisible(true);
	PANEL_BUTTON_GROUP_SETTINGS.setLeftMargin(331);
	PANEL_BUTTON_GROUP_NAV.setWidth(331);
	PANEL_BODY.setLeftMargin(331);
	BUTTON_NAV_SETTINGS_CLOSE.setVisible(true);
	BUTTON_NAV_SETTINGS_OPEN.setVisible(false);
	if ((g_settings_on)) {
		PANEL_BUTTON_GROUP_SETTINGS.setWidth(201);
	} else {
		PANEL_BUTTON_GROUP_SETTINGS.setWidth(170);
	}
	CROSSTAB.setLeftMargin(CROSSTAB.getLeftMargin()+1);
    CROSSTAB.setLeftMargin(CROSSTAB.getLeftMargin()-1);
    
    CROSSTAB_SPLIT.setLeftMargin(CROSSTAB.getLeftMargin()+1);
    CROSSTAB_SPLIT.setLeftMargin(CROSSTAB.getLeftMargin()-1); 

}
else {
	PANEL_BODY_LEFT.setVisible(false);
	PANEL_BUTTON_GROUP_SETTINGS.setLeftMargin(31);
	PANEL_BUTTON_GROUP_NAV.setWidth(31);
	PANEL_BODY.setLeftMargin(0);
	BUTTON_NAV_SETTINGS_CLOSE.setVisible(false);
	BUTTON_NAV_SETTINGS_OPEN.setVisible(true);
	if ((g_settings_on)) {
		PANEL_BUTTON_GROUP_SETTINGS.setWidth(170);
	}
	CROSSTAB.setLeftMargin(CROSSTAB.getLeftMargin()+1);
    CROSSTAB.setLeftMargin(CROSSTAB.getLeftMargin()-1);

    CROSSTAB_SPLIT.setLeftMargin(CROSSTAB.getLeftMargin()+1);
    CROSSTAB_SPLIT.setLeftMargin(CROSSTAB.getLeftMargin()-1);
}]]></bi:value>
					</bi:property>
				</bi:property>
				<bi:property name="FUNCTION" index="1">
					<bi:property name="NAME" value="display_buttons"/>
					<bi:property name="FUNCTION_BODY">
						<bi:value><![CDATA[BUTTON_CHART.setCSSClass("ICON_CHART");
BUTTON_TABLE.setCSSClass("ICON_TABLE");
BUTTON_CHART_TABLE.setCSSClass("ICON_CHART_TABLE");

if (g_display_mode == "TABLE") {
	BUTTON_TABLE.setCSSClass(BUTTON_TABLE.getCSSClass()+ " B_ACTIVE");
	CHART_SETTINGS.setVisible(false);
	CHART_SETTINGS.setBottomMargin(0);
	TABLE_SETTINGS.setVisible(true);
	TABLE_SETTINGS.setTopMargin(0);		
}
else if (g_display_mode == "CHART") {
	BUTTON_CHART.setCSSClass(BUTTON_CHART.getCSSClass()+ " B_ACTIVE");	
	CHART_SETTINGS.setVisible(true);
	CHART_SETTINGS.setBottomMargin(0);
	TABLE_SETTINGS.setVisible(false);
	TABLE_SETTINGS.setTopMargin(0);

}

else {
	BUTTON_CHART_TABLE.setCSSClass(BUTTON_CHART_TABLE.getCSSClass()+ " B_ACTIVE");	
	CHART_SETTINGS.setVisible(true);
	CHART_SETTINGS.setBottomMargin(405);
	TABLE_SETTINGS.setVisible(true);
	TABLE_SETTINGS.setTopMargin(405);	

}]]></bi:value>
					</bi:property>
				</bi:property>
				<bi:property name="FUNCTION" index="2">
					<bi:property name="NAME" value="filter_bar_action"/>
					<bi:property name="FUNCTION_BODY">
						<bi:value><![CDATA[if (g_filter_on) {
	BODY.setTopMargin(260);
	PANEL_BODY_DIALOG.setTopMargin(260);

	BUTTON_HEADER_FILTER.setTooltip(TEXT_POOL.HideFilterBar);
	PANEL_FILTER_AREA.setVisible(true);
	
	BUTTON_HEADER_FILTER.setCSSClass("B_ACTIVE ICON_FILTER");

}
else {
	BODY.setTopMargin(61);
	PANEL_BODY_DIALOG.setTopMargin(60);
	PANEL_FILTER_AREA.setVisible(false);
	BUTTON_HEADER_FILTER.setTooltip(TEXT_POOL.ShowFilterBar);
	BUTTON_HEADER_FILTER.setCSSClass("ICON_FILTER");	

}]]></bi:value>
					</bi:property>
				</bi:property>
				<bi:property name="FUNCTION" index="3">
					<bi:property name="NAME" value="chart_change_action"/>
					<bi:property name="FUNCTION_BODY">
						<bi:value><![CDATA[var l_chart_max = 0.0;
var l_chart_min = 0.0;


	if (chart_y_axis_scaling) {
		l_chart_max = Convert.stringToFloat(chart_min);
		l_chart_min = Convert.stringToFloat(chart_max);
		CHART.setAxisScaling(ChartAxisScaling.Y_AXIS, l_chart_min, l_chart_max);
		CHART_SPLIT.setAxisScaling(ChartAxisScaling.Y_AXIS, l_chart_min, l_chart_max);
	} 
	else {
		CHART.removeAxisScaling(ChartAxisScaling.Y_AXIS);
		CHART_SPLIT.removeAxisScaling(ChartAxisScaling.Y_AXIS);
	}

	if (chart_x_axis_scaling) {
		l_chart_max = Convert.stringToFloat(chart_min);
		l_chart_min = Convert.stringToFloat(chart_max);
		CHART.setAxisScaling(ChartAxisScaling.X_AXIS, l_chart_min, l_chart_max);
		CHART_SPLIT.setAxisScaling(ChartAxisScaling.X_AXIS, l_chart_min, l_chart_max);
	} 
	else {
		CHART.removeAxisScaling(ChartAxisScaling.X_AXIS);
		CHART_SPLIT.removeAxisScaling(ChartAxisScaling.X_AXIS);
	}]]></bi:value>
					</bi:property>
					<bi:property name="INPUT_PARAMETERS" type="ORDEREDLIST">
						<bi:property name="INPUT_PARAMETER" index="0">
							<bi:property name="NAME" value="chart_min"/>
							<bi:property name="PARAMETER_TYPE" value="String"/>
						</bi:property>
						<bi:property name="INPUT_PARAMETER" index="1">
							<bi:property name="NAME" value="chart_max"/>
							<bi:property name="PARAMETER_TYPE" value="String"/>
						</bi:property>
						<bi:property name="INPUT_PARAMETER" index="2">
							<bi:property name="NAME" value="chart_y_axis_scaling"/>
							<bi:property name="PARAMETER_TYPE" value="boolean"/>
						</bi:property>
						<bi:property name="INPUT_PARAMETER" index="3">
							<bi:property name="NAME" value="chart_x_axis_scaling"/>
							<bi:property name="PARAMETER_TYPE" value="boolean"/>
						</bi:property>
					</bi:property>
				</bi:property>
				<bi:property name="FUNCTION" index="4">
					<bi:property name="NAME" value="settings_panel_action"/>
					<bi:property name="FUNCTION_BODY">
						<bi:value><![CDATA[if (g_settings_on) {
	SETTINGS.setVisible(true);
	BUTTON_CHART_SETTINGS_OPEN.setVisible(false);
	BUTTON_CHART_SETTINGS_CLOSE.setVisible(true);
	PANEL_DETAILS_CROSSTAB.setLeftMargin(201);
	PANEL_DETAILS_CHART.setLeftMargin(201);
	PANEL_DETAILS_CHART_TABLE.setLeftMargin(201); 
	if ((g_navigation_on)) {
		PANEL_BUTTON_GROUP_SETTINGS.setWidth(201);
	} else {
		PANEL_BUTTON_GROUP_SETTINGS.setWidth(170);
	}
	CROSSTAB.setLeftMargin(CROSSTAB.getLeftMargin()+1);
    CROSSTAB.setLeftMargin(CROSSTAB.getLeftMargin()-1);

    CROSSTAB_SPLIT.setLeftMargin(CROSSTAB.getLeftMargin()+1);
    CROSSTAB_SPLIT.setLeftMargin(CROSSTAB.getLeftMargin()-1);
}
else {
SETTINGS.setVisible(false);
BUTTON_CHART_SETTINGS_OPEN.setVisible(true);
BUTTON_CHART_SETTINGS_CLOSE.setVisible(false);

PANEL_BUTTON_GROUP_SETTINGS.setWidth(30);
PANEL_DETAILS_CROSSTAB.setLeftMargin(0);
PANEL_DETAILS_CHART.setLeftMargin(0);
PANEL_DETAILS_CHART_TABLE.setLeftMargin(0); 

	CROSSTAB.setLeftMargin(CROSSTAB.getLeftMargin()+1);
    CROSSTAB.setLeftMargin(CROSSTAB.getLeftMargin()-1);

    CROSSTAB_SPLIT.setLeftMargin(CROSSTAB.getLeftMargin()+1);
    CROSSTAB_SPLIT.setLeftMargin(CROSSTAB.getLeftMargin()-1);

}]]></bi:value>
					</bi:property>
				</bi:property>
				<bi:property name="FUNCTION" index="5">
					<bi:property name="NAME" value="setfilterQuarter"/>
					<bi:property name="FUNCTION_BODY">
						<bi:value><![CDATA[// Q1
	if (BUTTON_Q1.getCSSClass() == "B_ACTIVE" && BUTTON_Q2.getCSSClass() == "B_ACTIVE" && BUTTON_Q3.getCSSClass() == "B_ACTIVE" && BUTTON_Q4.getCSSClass() == "B_ACTIVE") {		
		DS.setFilter("0CALQUART1", ["1", "2", "3", "4"]);
	}
	
	else if (BUTTON_Q1.getCSSClass() == "B_ACTIVE" && BUTTON_Q2.getCSSClass() == "B_ACTIVE" && BUTTON_Q3.getCSSClass() == "B_ACTIVE") {
		DS.setFilter("0CALQUART1", ["1", "2", "3"]);
	}
	
	else if (BUTTON_Q1.getCSSClass() == "B_ACTIVE" && BUTTON_Q2.getCSSClass() == "B_ACTIVE") {		
		DS.setFilter("0CALQUART1", ["1", "2"]);

	}
	
	else if (BUTTON_Q1.getCSSClass() == "B_ACTIVE" && BUTTON_Q3.getCSSClass() == "B_ACTIVE") {		
		DS.setFilter("0CALQUART1", ["1", "3"]);
	}
	
	else if (BUTTON_Q1.getCSSClass() == "B_ACTIVE" && BUTTON_Q4.getCSSClass() == "B_ACTIVE") {		
		DS.setFilter("0CALQUART1", ["1", "4"]);
	}
	else if (BUTTON_Q1.getCSSClass() == "B_ACTIVE" ) {		
		DS.setFilter("0CALQUART1", "1");
	}
	
// Q2
	else if (BUTTON_Q2.getCSSClass() == "B_ACTIVE" && BUTTON_Q3.getCSSClass() == "B_ACTIVE" && BUTTON_Q4.getCSSClass() == "B_ACTIVE") {		
		DS.setFilter("0CALQUART1", ["2", "3", "4"]);
	}
	
	else if (BUTTON_Q2.getCSSClass() == "B_ACTIVE" && BUTTON_Q3.getCSSClass() == "B_ACTIVE" ) {		
		DS.setFilter("0CALQUART1", ["2", "3"]);
	}
	
	else if (BUTTON_Q2.getCSSClass() == "B_ACTIVE" && BUTTON_Q4.getCSSClass() == "B_ACTIVE" ) {		
		DS.setFilter("0CALQUART1", ["2", "4"]);
	}
	else if (BUTTON_Q2.getCSSClass() == "B_ACTIVE" ) {		
		DS.setFilter("0CALQUART1", "2");
	}

// Q3	
	else if (BUTTON_Q3.getCSSClass() == "B_ACTIVE" && BUTTON_Q4.getCSSClass() == "B_ACTIVE" ) {		
		DS.setFilter("0CALQUART1", ["3", "4"]);
	}
	
	else if (BUTTON_Q3.getCSSClass() == "B_ACTIVE") {		
		DS.setFilter("0CALQUART1", "3");
	}
	
// Q4
	else if (BUTTON_Q4.getCSSClass() == "B_ACTIVE" ) {		
		DS.setFilter("0CALQUART1", "4");
	}
	
	else {		
		DS.clearFilter("0CALQUART1");
	}
	
]]></bi:value>
					</bi:property>
				</bi:property>
			</bi:property>
		</bi:component>
	</bi:component>
